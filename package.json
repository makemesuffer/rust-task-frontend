{
    "name": "frontend-template",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "lint-staged",
        "test": "jest",
        "test:watch": "npm run test -- --watch",
        "prettier:check": "prettier --check src",
        "prettier:fix": "prettier --write src",
        "eslint": "eslint src",
        "eslint:fix": "eslint src --fix"
    },
    "dependencies": {
        "autoprefixer": "^10.4.4",
        "classnames": "^2.3.1",
        "daisyui": "^2.11.0",
        "next": "12.1.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "tailwindcss": "^3.0.23",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.2",
        "@testing-library/react": "^12.1.4",
        "@types/jest": "^27.4.1",
        "@types/node": "17.0.21",
        "@types/react": "17.0.41",
        "@types/react-test-renderer": "^17.0.1",
        "@typescript-eslint/eslint-plugin": "^5.15.0",
        "babel-jest": "^27.5.1",
        "eslint": "8.11.0",
        "eslint-config-next": "12.1.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^7.0.4",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^27.5.1",
        "lint-staged": "^12.3.7",
        "prettier": "2.6.0",
        "react-test-renderer": "^17.0.2",
        "typescript": "4.6.2"
    },
    "lint-staged": {
        "*.{js,ts,tsx, jsx}": [
            "eslint --quiet --fix",
            "npm run test"
        ],
        "*.{json,md,html,js,jsx,ts,tsx}": [
            "prettier --write"
        ]
    }
}
